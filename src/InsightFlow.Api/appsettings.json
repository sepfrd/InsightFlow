{
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.json",
          "rollingInterval": "Day",
          "rollOnFileSizeLimit": true,
          "formatter": "Serilog.Formatting.Json.JsonFormatter",
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} {Level:u3}] Message: {Message:lj} | Properties: {Properties:j} | SourceContext: {SourceContext}{NewLine}{Exception}"
        }
      },
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} {Level}]{NewLine}Message: {Message:lj}{NewLine}SourceContext: {SourceContext}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId",
      "WithExceptionDetails"
    ]
  },
  "SqlServerConnectionString": "Server=localhost,1433;Database=InsightFlow_V2;User Id={Username};Password={Password};Trust Server Certificate=True;Encrypt=False",
  "SqliteConnectionString": "Data Source=./insight_flow.db",
  "DatabaseProvider": "Sqlite",
  "ApplicationInformation": {
    "Name": "InsightFlow",
    "Version": "2.0.0",
    "ServerUrl": "http://localhost",
    "ClientUrl": "http://localhost",
    "DeveloperName": "Sepehr Foroughi Rad",
    "DeveloperUrl": "https://github.com/sepehr-frd",
    "DeveloperEmail": "sepfrd@outlook.com"
  },
  "JwtOptions": {
    "PrivateKey": "-----BEGIN PRIVATE KEY-----\\n-----END PRIVATE KEY-----",
    "PublicKey": "-----BEGIN PUBLIC KEY-----\\n-----END PUBLIC KEY-----",
    "TokenExpirationDurationMinutes": 1440
  },
  "PaginationOptions": {
    "MinimumAllowedPageSize": 1,
    "MaximumAllowedPageSize": 100
  },
  "ProfileImageMaximumAllowedBytes": 2097152,
  "RateLimitersConfiguration": {
    "FixedWindowRateLimiterOptions": {
      "PermitLimit": 5,
      "WindowSeconds": 120,
      "QueueLimit": 0,
      "QueueProcessingOrder": 0,
      "AutoReplenishment": true
    },
    "TokenBucketRateLimiterOptions": {
      "ReplenishmentPeriodSeconds": 10,
      "TokensPerPeriod": 10,
      "AutoReplenishment": true,
      "TokenLimit": 100,
      "QueueProcessingOrder": 0,
      "QueueLimit": 0
    },
    "ConcurrencyLimiterOptions": {
      "PermitLimit": 100,
      "QueueProcessingOrder": 0,
      "QueueLimit": 0
    }
  }
}